#include <iostream>
using namespace std;

int ans(int a)
{
    // base condition
    if (a == 0)
    {
        return 1;
    }
    return a * ans(a - 1);
}

int main()
{
    int num = 5;

    // cout << "Enter the number: ";

    // cin >> num;

    cout << "The factor of " << num << " is : " << ans(num) << endl;

    return 0;
}
